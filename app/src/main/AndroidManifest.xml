<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mooveop.mooveopapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_Coarse_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MooveopApp">
        <activity
            android:name=".TestIssueActivity"
            android:exported="true"
            android:label="@string/title_activity_test_issue"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".GetRiderQuoteActivity"
            android:exported="true"
            android:label="@string/title_activity_get_rider_quote"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".GetRiderQuote"
            android:exported="true"
            android:label="@string/title_activity_get_rider_quote"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".Signin"
            android:exported="true"
            android:label="@string/title_activity_signin"
            android:theme="@style/Theme.MooveopApp.NoActionBar">

        </activity>
        <activity
            android:name=".DeliveryActivity"
            android:exported="true"
            android:label="@string/title_activity_delivery"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".ViewCartActivity"
            android:exported="true"
            android:label="@string/title_activity_view_cart"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".MenuActivity"
            android:exported="true"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.MooveopApp.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MooveopApp.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>